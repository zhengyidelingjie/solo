page {
  height: 100%;
}

.header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 96rpx;
  z-index: 10;
  overflow: hidden;
}

scroll-view {
  height: 100%;
}

.head {
  position: relative;
  height: 96rpx;
}

.header.scrolled {
  background: #fff;
}

.header.scrolled:after {
  position: absolute;
  bottom: 0px;
  left: 0px;
  right: 0px;
  width: 200%;
  height: 1px;
  background: #dedede;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.collect {
  right: 24rpx;
  display: block;
