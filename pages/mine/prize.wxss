.container {
  width: 100%;
}

.prize {
  width: 94%;
  padding: 0 12px 10px;
}

.prize-li {
  width: 100%;
  margin-top: 10px;
  background: url(http://appuat.huihuishenghuo.com/img/mine/myprize.png) no-repeat center top / 100% auto;
}

.prize-title {
  padding: 13px 12px;
  overflow: hidden;
}

.prize-h {
  color: #e95377;
  line-height: 20px;
  font-size: 34rpx;
  font-weight: 700;
  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
}

.prize-time {
  background: #e95377;
  color: #fff;
  height: 30px;
  padding: 0 12px;
  line-height: 30px;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-size: 28rpx;
}

.time {
  float: left;
}

.time-type {
  float: right;
}
